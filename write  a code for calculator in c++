#include<iostream>
using namespace std;

int main()
{
    int b,c,d,e;
    float a,f;
    cout<<"Enter the value of a: "<<endl;
    cin>>a;
    cout<<"Enter the value of b: "<<endl;
    cin>>b;
    c = a+b;
    cout<<"Sum is: "<<c<<endl;
    d = a-b;
    cout<<"Difference is: "<<d<<endl;
    e = a*b;
    cout<<"Produt is: "<<e<<endl;
    f = a/b;
    cout<<"Division is: "<<f<<endl;
}
